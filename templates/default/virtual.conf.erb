server {
             listen *:80 ;
             server_name <%= @server_name %>;

             access_log <%= @access_log %>;

             location /kibana/ {
                       root /var/www/html;
                       index index.html index.htm;
             }

             location / {
             proxy_pass <%= @proxy_pass %>;
             proxy_read_timeout 90;
             }
 }